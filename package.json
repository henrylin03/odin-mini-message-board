{
	"name": "odin-mini-message-board",
	"version": "1.1.0",
	"description": "Building a mini message board using ExpressJS, with templating in EJS.",
	"main": "app.js",
	"scripts": {
		"start": "node --watch app.js",
		"prepare": "husky",
		"seed-db": "node db/populatedb.js"
	},
	"lint-staged": {
		"*": [
			"biome check --write --no-errors-on-unmatched"
		]
	},
	"keywords": [
		"message board",
		"full stack web app",
		"node",
		"ExpressJS"
	],
	"author": "Henry Lin",
	"license": "MIT",
	"type": "commonjs",
	"dependencies": {
		"dotenv": "^17.2.3",
		"ejs": "^3.1.10",
		"express": "^5.2.1",
		"pg": "^8.16.3"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.8",
		"husky": "^9.1.7",
		"lint-staged": "^16.2.7"
	}
}
